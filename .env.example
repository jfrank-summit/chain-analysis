CONSENSUS_RPC_WS=wss://rpc.mainnet.autonomys.xyz/ws
AUTO_EVM_RPC_WS=wss://auto-evm.mainnet.autonomys.xyz/ws
DATA_DIR=./data
LOG_LEVEL=info

# Batching thresholds for Parquet writes
# Flush when buffer reaches this many rows
WRITE_BATCH_ROWS=5000
# Or when this many milliseconds elapse since last flush
WRITE_BATCH_MS=60000

# Backfill settings
K_CONSENSUS=64
K_AUTO_EVM=64
# Optionally constrain backfill to a height range (inclusive)
# BACKFILL_START=0
# BACKFILL_END=0

