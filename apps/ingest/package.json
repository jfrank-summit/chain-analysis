{
  "name": "@chain-analysis/ingest",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "main": "dist/bin.js",
  "types": "src/bin.ts",
  "exports": {
    ".": {
      "types": "./src/bin.ts",
      "development": "./src/bin.ts",
      "default": "./dist/bin.js"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "DOTENV_CONFIG_PATH=../../.env tsx watch src/bin.ts",
    "start": "DOTENV_CONFIG_PATH=../../.env node --enable-source-maps dist/bin.js",
    "dev:backfill": "DOTENV_CONFIG_PATH=../../.env tsx src/bin.ts backfill"
  },
  "dependencies": {
    "@chain-analysis/chain": "workspace:*",
    "@chain-analysis/config": "workspace:*",
    "@chain-analysis/storage": "workspace:*"
  }
}
